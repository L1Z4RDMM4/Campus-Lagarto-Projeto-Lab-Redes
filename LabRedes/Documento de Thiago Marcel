node n1 {
    type router
    model host
    network-config {
	hostname n1
	!
	interface eth1
	 ip address 192.0.3.10/24
	 ipv6 address 2001:db8:1::10/64
	!
	interface eth0
	 ip address 192.0.2.10/24
	 ipv6 address 2001:db8:0::10/64
	!
    }
    canvas c1
    iconcoords {412.0 39.0}
    labelcoords {412.0 71.0}
    interface-peer {eth0 n2}
    interface-peer {eth1 n3}
}

