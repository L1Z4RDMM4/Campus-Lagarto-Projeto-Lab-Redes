node n6 {
    type wlan
    network-config {
	hostname wlan6
	!
	interface wireless
	 ip address 192.0.6.0/32
	 ipv6 address 2001:db8:4::0/128
	!
	mobmodel
	coreapi
	basic_range
	!
    }
    custom-config {
	custom-config-id basic_range
	custom-command {3 3 9 9 9}
	config {
	range=275
	bandwidth=54000000
	jitter=0
	delay=20000
	error=0
	}
    }
    canvas c1
    iconcoords {130.0 299.0}
    labelcoords {130.0 331.0}
    interface-peer {e0 n4}
}

